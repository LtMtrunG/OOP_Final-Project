<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="920.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guifinal.AdminCustomerController">
    <children>
        <VBox prefHeight="800.0" prefWidth="920.0" style="-fx-background-color: #F4F4FC;">
            <children>
                <Label text="Customer">
                    <font>
                        <Font name="Cambria Bold" size="36.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="20.0" top="30.0" />
                    </VBox.margin>
                </Label>
                <HBox alignment="BOTTOM_LEFT" prefHeight="71.0" prefWidth="880.0">
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                    </VBox.margin>
                    <children>
                        <HBox alignment="CENTER" maxHeight="-Infinity" prefHeight="27.0" prefWidth="610.0" style="-fx-border-radius: 50; -fx-border-color: #878787; -fx-background-color: #fff; -fx-background-radius: 50;">
                            <children>
                                <TextField prefHeight="25.0" prefWidth="530.0" promptText="Search" style="-fx-background-color: transparent;" />
                                <Button mnemonicParsing="false" prefHeight="37.0" prefWidth="79.0" style="-fx-background-color: #828282; -fx-background-radius: 50;" text="Search" textFill="WHITE">
                                    <font>
                                        <Font name="Calibri Bold" size="10.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                    <VBox.margin>
                                        <Insets left="20.0" right="20.0" />
                                    </VBox.margin>
                                </Button>
                            </children>
                        </HBox>
                  <Label text="Customer Type:">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Candara Bold" size="16.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="cbCustomerType" prefWidth="120.0" style="-fx-background-color: #FFF; -fx-background-radius: 50; -fx-border-color: #878787; -fx-border-radius: 50;">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </ChoiceBox>
                    </children>
                    <padding>
                        <Insets top="30.0" />
                    </padding></HBox>
                <TableView prefHeight="595.0" prefWidth="880.0" style="-fx-background-radius: 10;">
                    <columns>
                        <TableColumn prefWidth="91.0" text="ID" />
                        <TableColumn prefWidth="221.0" text="Name" />
                        <TableColumn prefWidth="215.0" text="Address" />
                        <TableColumn minWidth="0.0" prefWidth="94.0" text="Phone" />
                        <TableColumn prefWidth="176.0" text="List of Rental" />
                  <TableColumn minWidth="0.0" prefWidth="82.0" text="Type" />
                    </columns>
                    <VBox.margin>
                        <Insets left="20.0" right="20.0" top="20.0" />
                    </VBox.margin>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
